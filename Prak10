package praktikum10;

/**
 *
 * @author USER
 */
public class Main {
    public static void main(String[] args) {
        OperasiHitung tambah = new Penjumlahan();
        OperasiHitung kurang = new Pengurangan();
        OperasiHitung kali = new Perkalian();
        OperasiHitung bagi = new Pembagian();
        
        System.out.println("Hasil penjumlahan: " + tambah.hitung(10, 5));  
        System.out.println("Hasil pengurangan: " + kurang.hitung(10, 5));  
        System.out.println("Hasil perkalian: " + kali.hitung(10, 5));      
        System.out.println("Hasil pembagian: " + bagi.hitung(10, 5));      
    }
}
